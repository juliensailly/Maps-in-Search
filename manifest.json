{
    "manifest_version": 3,
    "name": "Maps in Search",
    "version": "1.0",
    "description": "Execute a function on new page open",
    "permissions": [
        "activeTab",
        "tabs"
    ],
    "host_permissions": [
        "*://*.google.com/*",
        "*://*.google.*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.google.com/*",
                "*://*.google.*"
            ],
            "js": [
                "script.js"
            ]
        }
    ]
}